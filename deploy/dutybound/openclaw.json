{
  "gateway": {
    "port": 18793,
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "token"
    }
  },
  "agents": {
    "defaults": {
      "workspace": "/home/node/.openclaw/workspace",
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "workspace": "/home/node/.openclaw/workspace"
      }
    ]
  },
  "skills": {
    "install": {
      "nodeManager": "npm"
    }
  }
}
